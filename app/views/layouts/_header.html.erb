<div id="header" class="round">
  <%= link_to logo, "http://www.toderistov.com" %>
  <span>
  <ul class="navigation round" >
    <li><%= link_to en_flag, :locale => 'en' %></li>
    <li><%= link_to mk_flag, :locale => 'mk' %></li>
     
  </ul>
  </span>
  <span>
  <ul class="navigation round">
    <li><%= link_to t(:home), root_path %></li>
    <% if signed_in? %>
    <li><%= link_to t(:users), users_path %></li>
    <li><%= link_to t(:profile), current_user %></li>
    <li><%= link_to t(:settings), edit_user_path(current_user) %></li>
    <% end %>
    <li><%= link_to t(:help), help_path %></li>
    <% if signed_in? %>
    <li><%= link_to t(:sign_out), signout_path, :method => :delete %></li>
    <% else %>
    <li><%= link_to t(:sign_in), signin_path %></li>
    <% end %>
  </ul>
  </span>
</div>
